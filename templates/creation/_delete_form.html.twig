<form method="post" action="{{ path('app_creation_delete', {'id': creation.id}) }}" onsubmit="return confirm('Êtes-vous sûr de vouloir supprimer cette création ?');" class="d-inline">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ creation.id) }}">
    <button class="btn btn-danger">Supprimer</button>
</form>
